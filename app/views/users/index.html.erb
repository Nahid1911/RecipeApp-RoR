<p style="color: green"><%= notice %></p>
<h1>Users</h1>
<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
    <p>
      <%= link_to "user recipies", user_recipes_path(user, @recipe) %>
    </p>
  <% end %>
</div>
<%= link_to "New user", new_user_path %>
